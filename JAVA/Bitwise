public class bitwise {
     public static void main(String[] args) {
       int x=5;
       int y=3;
//         and
       System.out.println(x&y);
//        or
         System.out.println(x|y);
//         xor
         System.out.println(x^y);
//         not
         System.out.println(~x);
    }
}
