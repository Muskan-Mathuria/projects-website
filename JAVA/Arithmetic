public class arthmetic {
    public   static void main(String[] args) {
//        ARTHEMETIC OPERATORS
        int a = 4;
        int b = 2;
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        System.out.println(a/b);
        System.out.println(a%b);

    }
}
