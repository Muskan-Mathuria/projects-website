public class Com {
    public static void main(String[] args) {
//        COMPARISON OPERATORS OR RELATIONAL OPERATORS
        int x=30;
        int y=78;
        System.out.println(x==y);
        System.out.println(x!=y);
        System.out.println(x>y);
        System.out.println(x<y);
        System.out.println(x>=y);
        System.out.println(x<=y);

    }
}
